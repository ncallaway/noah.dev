{
  "name": "noah.dev",
  "version": "1.0.0",
  "description": "Building theme file assets for the noah.dev wordpress site",
  "scripts": {
    "build-css": "npx sass assets/css/main.scss wp-content/themes/noah.dev/assets/main.min.css --style compressed",
    "watch-css": "npx sass assets/css/main.scss wp-content/themes/noah.dev/assets/main.min.css --style compressed --watch",
    "build-js": "webpack --config webpack.config.js"
  },
